{
	"name" : "leinad4mind/post_revisions",
	"type" : "phpbb-extension",
	"description" : "Stores each revision of a post every time a post is edited. It will only make these past revisions viewable to those users who have the necessary permissions.",
	"homepage" : "https://leinad4mind.top",
	"version" : "1.0.1",
	"time" : "2018-10-15",
	"license" : "GPL-2.0",
	"keywords" : [ "post", "revision" ]
	"authors" : [
		{
			"name" : "Leinad4Mind",
			"role" : "Developer"
		},
		{
			"name" : "Prime Halo",
			"role" : "Original Developer"
		}
	],
	"require" : {
		"php" : ">=5.4"
	},
	"extra" : {
		"display-name" : "Prime Post Revision",
		"soft-require" : {
			"phpbb/phpbb" : "3.2.*"
		},
		"version-check" : {
			"host" : "leinad4mind.top",
			"directory" : "/phpbb/",
			"filename" : "version.json"
		}
	},

}